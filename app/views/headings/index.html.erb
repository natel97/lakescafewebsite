<% @headings.each do |heading| %>

		<h1 class="page-header" id="<%=  heading.name %>" class="heading"><%=  heading.name %></h1>
		<%  @sections.each do |section| %>
		<% if section.heading_id == heading.id %>
		<div class="well well-lg">
		<h3 id="<%=  section.name %>" class="section container-fluid"><%= section.name %></h3>
		<p><%=section.description%></p>
		<table class="table table-striped">
			<tbody>
			
		
		<% @items.each do |item|%>
		<% if item.section_id == section.id %>
		<tr>		
		<td><%= item.name%></td>
		<td><%= item.description%></td>
		<% if item.price.present? %>
		<td style="text-align: right;">$<%= item.price%></td>
		<%end%>
		</tr>
		
		<% @subitems.each do |subitem|%>
		<% if subitem.item_id == item.id %>
		
		<tr>		
		<td style="font-style: italic; padding-left: 3em;"><%= subitem.name%></td>
		<td style="font-style: italic;"><%= subitem.description%></td>
		<td style="font-style: italic; text-align: right;">$<%= subitem.price%></td>
		</tr>
		
		<%end%>
		<%end%>
		<%end%>
		<%end%>
		</tbody>
		</table>
		</div>
		<%end%>
		<%end%>
		<br><br><br>
		<%end%>
		</div>
